<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TATM.SGCB" xmlns:local1="clr-namespace:TATM.SGCB" x:Class="TATM.SGCB.CellCtrl" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Viewbox StretchDirection="DownOnly">
        <Grid>
            <Image x:Name="Entity" Margin="16" Source="pack://siteoforigin:,,,/sgcb/theseus.png"/>
            <Image x:Name="Exit" HorizontalAlignment="Left" Height="274" Margin="16,13,0,0" VerticalAlignment="Top" Width="268" Grid.ColumnSpan="2" Source="pack://siteoforigin:,,,/sgcb/exit.png"/>
            <Canvas x:Name="RightWall" Background="Black" HorizontalAlignment="Right" Width="16"/>
            <Canvas x:Name="LeftWall" HorizontalAlignment="Left" Width="16" Background="Black"/>
            <Canvas x:Name="BottomWall" Background="Black" Height="16" VerticalAlignment="Bottom"/>
            <Canvas x:Name="TopWall" Background="Black" Height="16" VerticalAlignment="Top"/>
        </Grid>
    </Viewbox>
</UserControl>
