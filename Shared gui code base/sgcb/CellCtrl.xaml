<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TATM" xmlns:local1="clr-namespace:TATM.sgcb" x:Class="TATM.sgcb.CellCtrl" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Canvas x:Name="RightWall" HorizontalAlignment="Left" Margin="134,0,0,0" Width="16" Background="Black" Grid.Column="1"/>
        <Canvas x:Name="LeftWall" HorizontalAlignment="Left" Width="16" Background="Black"/>
        <Canvas x:Name="BottomWall" Margin="0,287,0,0" Background="Black" Grid.ColumnSpan="2"/>
        <Canvas x:Name="TopWall" Margin="0,0,0,287" Background="Black" Grid.ColumnSpan="2"/>
        <Image x:Name="Exit" HorizontalAlignment="Left" Height="264" Margin="16,18,0,0" VerticalAlignment="Top" Width="263" Grid.ColumnSpan="2" Source="pack://siteoforigin:,,,/sgcb/exit.png"/>
        <local1:EntityCtrl x:Name="Entity" Grid.ColumnSpan="2" HorizontalAlignment="Left" Height="264" Margin="21,18,0,0" VerticalAlignment="Top" Width="258"/>
    </Grid>
</UserControl>
