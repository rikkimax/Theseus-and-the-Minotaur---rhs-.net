<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TATM.SGCB" xmlns:local1="clr-namespace:TATM.SGCB" x:Class="TATM.SGCB.CellCtrl" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid ClipToBounds="True">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="38*"/>
            <ColumnDefinition Width="37*"/>
        </Grid.ColumnDefinitions>
        <Image x:Name="Exit" HorizontalAlignment="Left" Height="274" Margin="16,13,0,0" VerticalAlignment="Top" Width="268" Grid.ColumnSpan="2" Source="pack://siteoforigin:,,,/sgcb/exit.png"/>
        <Image x:Name="Entity" HorizontalAlignment="Left" Height="274" Margin="16,13,0,0" VerticalAlignment="Top" Width="268" Grid.ColumnSpan="2"/>
        <Canvas x:Name="RightWall" Margin="131.81,-10,0,10" Width="16" Background="Black" Grid.Column="1"/>
        <Canvas x:Name="LeftWall" HorizontalAlignment="Left" Width="16" Background="Black"/>
        <Canvas x:Name="BottomWall" Margin="0,287,0,0" Background="Black" Grid.ColumnSpan="2" Height="16"/>
        <Canvas x:Name="TopWall" Margin="0,0,0,287" Background="Black" Grid.ColumnSpan="2" Height="16"/>
    </Grid>
</UserControl>
